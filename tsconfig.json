    "target": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "noEmit": true,
    "strict": true,
    "lib": ["dom", "esnext"],
    "module": "esnext",
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "skipLibCheck": true,
    "rootDir": "src"
  },
}
